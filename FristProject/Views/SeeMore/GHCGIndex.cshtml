<body id="index">

    <!-- loading -->
    <aside id="loading" class="wrap">
        <div class="process-bg"></div>
        <div class="process"><span></span></div>
    </aside>

    <!-- swiper -->
    <div id="mySwiper" class="swiper-container">
        <div class="swiper-wrapper">
            <!-- IndexPage -->
            <aside class="swiper-slide swiper-no-swiping">
                <img class="img ani" src="~/Content/2019/1105/images/bg.png" swiper-animate-duration="0.5s" swiper-animate-delay="0s" swiper-animate-effect="fadeIn" />
                <div class="middle-wrap">
                    <img class="img ani" swiper-animate-duration="1.0s" swiper-animate-delay="0.5s" src="~/Content/2019/1105/images/page/1-1.png" swiper-animate-effect="fadeInDown" />
                    <img class="img ani" swiper-animate-duration="1.0s" swiper-animate-delay="1.0s" src="~/Content/2019/1105/images/page/1-2.png" swiper-animate-effect="fadeInLeft" />
                    <img class="img ani" swiper-animate-duration="1.0s" swiper-animate-delay="1.5s" src="~/Content/2019/1105/images/page/1-3.png" swiper-animate-effect="fadeInRight" />
                    <img class="img ani" swiper-animate-duration="1.0s" swiper-animate-delay="2.0s" src="~/Content/2019/1105/images/page/1-4.png" swiper-animate-effect="fadeInLeft" />
                    <form class="ani" swiper-animate-duration="1.0s" swiper-animate-delay="2.5s" swiper-animate-effect="fadeIn">
                        <ul>
                            <li class="user"><span></span><input type="text" maxlength="8" /></li>
                            <li class="phone"><span></span><input type="tel" maxlength="11" /></li>
                        </ul>
                        <div class="btns-flex">
                            <input type="button" class="btn-submit" />
                        </div>
                    </form>
                </div>
            </aside>
        </div>
    </div>
    <!-- 固定的 -->
    <div class="logo"></div>
    <!-- <div class="music music-ani"></div> -->
    <div class="flip-over flip-over-ani"></div>
    <!-- Alert -->
    <div id="alerts" class="alerts"></div>
    <div id="alertConfirm" class="alert-confirm"></div>
